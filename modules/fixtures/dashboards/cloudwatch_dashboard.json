{
  "widgets": [
    {
      "type": "metric",
      "width": 24,
      "height": 6,
      "properties": {
        "metrics": [
          [ "${metrics_namespace_var}", "LeasedAccounts", { "color": "#2ca02c", "label": "[last: $${LAST}] LeasedAccounts" } ],
          [ ".", "ReadyAccounts", { "color": "#1f77b4", "label": "[last: $${LAST}] ReadyAccounts" } ],
          [ ".", "NotReadyAccounts", { "color": "#ff7f0e", "label": "[last: $${LAST}] NotReadyAccounts" } ],
          [ ".", "OrphanedAccounts", { "color": "#d62728" ,"label": "[last: $${LAST}] OrphanedAccounts" } ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "title" : "Account Pool",
        "region": "us-east-1",
        "stat": "Average",
        "period": 60
      }
    },
    {
      "type": "metric",
      "width": 24,
      "height": 6,
      "properties": {
        "metrics": [
          [ "AWS/ApiGateway", "5XXError", "ApiName", "${api_name}", "Stage", "${api_stage_name}", { "color": "#d62728" } ],
          [ ".", "4XXError", ".", ".", ".", "." ],
          [ ".", "Count", ".", "." ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "title": "API Gateway",
        "region": "${region}",
        "legend": {
          "position": "bottom"
        },
        "stat": "Sum",
        "period": 300
      }
    },
    {
      "type": "metric",
      "width": 12,
      "height": 6,
      "properties": {
        "view": "timeSeries",
        "stacked": false,
        "metrics": [
          [ "AWS/CodeBuild", "SucceededBuilds", "ProjectName", "${codebuild_name}", { "color": "#1f77b4" } ],
          [ ".", "FailedBuilds", ".", ".", { "color": "#d62728" } ]
        ],
        "region": "${region}",
        "period": 300,
        "title": "resets codebuild",
        "stat": "Sum"
      }
    },
    {
      "type": "log",
      "width": 6,
      "height": 6,
      "properties": {
        "query": "SOURCE '/aws/codebuild/${codebuild_name}' | FIELDS @message, @logStream, @timestamp\n| filter @message ~= \": # Child Account\"\n| parse @message \"*: # Child Account\" as account\n| display account, @logStream, @timestamp",
        "region": "${region}",
        "stacked": false,
        "view": "table",
        "title": "Recent Resets"
      }
    },
    {
      "type": "log",
      "width": 6,
      "height": 6,
      "properties": {
        "query": "SOURCE '/aws/codebuild/${codebuild_name}' | FIELDS @message, @logStream, @timestamp\n| filter @message ~= \": # Child Account\"\n| parse @message \"*: # Child Account\" as account\n| display account, @logStream, @timestamp\n| stats count() by account as resetNum\n| sort resetNum asc",
        "region": "${region}",
        "stacked": false,
        "title": "Most Reset Accounts",
        "view": "table"
      }
    },
    {
      "type": "log",
      "width": 24,
      "height": 6,
      "properties": {
        "query": "SOURCE '/aws/codebuild/${codebuild_name}' | ${error_scraper_query}",
        "region": "us-east-1",
        "stacked": false,
        "view": "table",
        "title": "resets codebuild error scraper"
      }
    },
    {
      "type": "metric",
      "width": 12,
      "height": 6,
      "properties": {
        "metrics": [
          [ "AWS/Lambda", "Invocations", "FunctionName", "${accounts_lambda_name}", { "color": "#1f77b4" } ],
          [ ".", "Errors", ".", ".", { "color": "#d62728" } ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "region": "${region}",
        "title": "${accounts_lambda_name} λ",
        "stat": "Sum",
        "period": 300
      }
    },
    {
      "type": "log",
      "width": 12,
      "height": 6,
      "properties": {
